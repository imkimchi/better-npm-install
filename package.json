{
  "name": "better-npm-install",
  "version": "1.1.3",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "debug": "nodemon --exec babel-node src/index.js app /Users/philipyoonshin_spryfit/spryfit-node",
